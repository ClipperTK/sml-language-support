{
    "name": "sml-language-support",
    "main": "lib/sml-language-support",
    "version": "0.0.1",
    "description": "Language Support for Standard ML",
    "keywords": [],
    "activationCommands": {
        "atom-workspace": "sml-language-support:run"
    },
    "scripts": {
        "grammar": "tsc -w -p scripts/grammar && scripts/grammar/grammar.js"
    },
    "activationHooks": ["sml-language-support:grammar-used"],
    "repository": "https://github.com/SimonSpies/sml-language-support.git",
    "license": "MIT",
    "engines": {
        "atom": ">=1.0.0 <2.0.0"
    },

    "dependencies": {},
    "configSchema": {
        "mosml": {
            "type": "string",
            "default": "mosml",
            "description": "The place where your MoscowML interpreter lives."
        }
    },
    "providedServices": {
        "autocomplete.provider": {
            "versions": {
                "2.0.0": "provideAutocompletion"
            }
        }
    }
}